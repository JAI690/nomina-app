

<div class="container p-4">
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Nombre</th>
                <th>NSS</th>
                <th>RFC</th>
                <th>CURP</th>
                <th>Direccion</th>
                <th>Puesto</th>
                <th>Sueldo IMSS</th>
                <th>Patron</th>
                <th>Fecha Ingreso</th>
            </tr>
        </thead>
        <tbody>
        {{#each trabajadores}}
        <tr>
            <td>{{nombre}}</td>
            <td>{{NSS}}</td>
            <td>{{RFC}}</td>
            <td>{{CURP}}</td>
            <td>{{direccion}}</td>
            <td>{{puesto}}</td>
            <td>{{sueldoIMSS}}</td>
            <td>{{patron}}</td>
            <td>{{fechaIngreso}}</td>

        </tr>
        {{/each}}
        </tbody>
    </table>
</div>